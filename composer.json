{
  "config": {
        "github-protocols": ["https", "git"],
        "process-timeout": 900
  },

  "repositories": [
    {
        "type": "composer",
        "url": "http://packages.zendframework.com/"
    },
    {
        "type": "package",
        "package": {
           "name": "highcharts",
            "version": "4.0.4",
            "source": {
                "url": "git://github.com/highslide-software/highcharts.com",
                "type": "git",
                "reference": "tags/v4.0.4"
            }
        }
    }
  ],
  "require": {
        "php": ">=5.4.0",
        "highcharts": "4.0.4",
        "zendframework/zendframework": "2.2.0",
        "doctrine/common": "2.3.0",
        "zendframework/zendpdf": "2.0.2",
        "dino/dompdf-module": "dev-master",
        "zendframework/zendservice-recaptcha": "2.0.1",
        "doctrine/doctrine-orm-module": "0.7.0",
        "zf-commons/zfc-rbac": "dev-master",
        "snapshotpl/zf-snap-geoip": "2.*"
  },
  "scripts": {
        "post-install-cmd": [
            "ZfSnapGeoip\\Composer\\ScriptHandler::downloadData"
        ],
        "post-update-cmd": [
            "ZfSnapGeoip\\Composer\\ScriptHandler::downloadData"
        ]
  }
}
